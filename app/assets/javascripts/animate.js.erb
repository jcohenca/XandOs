    clearGraph();
    plot1 = $.jqplot('chart1', [storedData],
        { axes:{xaxis:{min:0, max: graphWidth, showTicks: false}, yaxis:{min:Math.min.apply(Math,finalSearch.map(function(o){return o.expected_points;})), max: Math.max.apply(Math,finalSearch.map(function(o){return o.expected_points;}))}}});

}

function clearGraph() {

